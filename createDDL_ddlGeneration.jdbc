CREATE TABLE COMMENT (CREATED DATE, HEADER VARCHAR(255), ID BIGINT GENERATED BY DEFAULT AS IDENTITY NOT NULL, TEXT VARCHAR(255), author_code VARCHAR(255), author_name VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE ISMA.USERT (ID BIGINT GENERATED BY DEFAULT AS IDENTITY NOT NULL, CODE VARCHAR(255), NAME VARCHAR(255), USERNAME VARCHAR(255), USERPWD VARCHAR(255), PRIMARY KEY (ID))
