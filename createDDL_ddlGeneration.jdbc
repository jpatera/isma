CREATE TABLE COMMENT (CREATED DATE, HEADER VARCHAR(255), ID BIGINT NOT NULL, TEXT VARCHAR(255), author_code VARCHAR(255), author_name VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE usr (ID BIGINT NOT NULL, CODE VARCHAR(255), FULLNAME VARCHAR(255), LOGINNAME VARCHAR(255), LOGINPWD VARCHAR(255), PRIMARY KEY (ID))
CREATE SEQUENCE SEQ_GEN_SEQUENCE INCREMENT BY 50 START WITH 50
